<?xml version="1.0"?>
<odoo>
	<data>
		<record id="view_excel_report_result_wizard" model="ir.ui.view">
			<field name="name">excel.report.result.wizard</field>
			<field name="model">excel.report.result.wizard</field>
			<field name="arch" type="xml">
				<form string="Genrated xls File">
					<group>
						<field name="file_name" invisible="1"/>
						<field name="excel_file" readonly="1" filename="file_name"/>
					</group>
					 <footer>
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
	 	</record>
	</data>
</odoo>
